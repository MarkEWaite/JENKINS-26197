<project name="JENKINS-32577" default="info" basedir=".">

  <target name="info" description="Show JENKINS-32577 not a bug">
    <echo>java version is ${java.version}</echo>
    <buildnumber/>
    <echo>Build number was ${build.number}</echo>
    <exec executable="git">
      <arg value="add"/>
      <arg value="build.number"/>
      <arg value="."/>
    </exec>
    <exec executable="git">
      <arg value="commit"/>
      <arg value="-m"/>
      <arg value="Increment build number from ant script - was ${build.number}"/>
      <arg value="build.number"/>
    </exec>
  </target>

</project>
